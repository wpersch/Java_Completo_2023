import java.util.Scanner;

public class Ex06 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Numbers Range");
		System.out.print("Enter a real number between 0 and 100: ");
		double number = sc.nextDouble();
		
		if (number >= 0 || number <= 25) {
			System.out.println("Range [0,25]");
		} else if (number <= 50) {
			System.out.println("Range (25,50]");

		} else if(number <= 75) {
			System.out.println("Range (50,75]");
			
		} else if (number <= 100){
			System.out.println("Range (75,100]");
			
		} else {
			System.out.println("Out of range");
		}
		
		sc.close();
	}

}
